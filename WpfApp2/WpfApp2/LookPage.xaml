<Window x:Class="WpfApp2.LookPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Title="LookPage" Height="450" Width="700">
    <Grid Background="#F8FAE5">
        <Border MinWidth="450" Margin="10" MaxWidth="700" Padding="30" CornerRadius="20" Background="White">
            <StackPanel>
                <Grid Margin="0 10">
                    <TextBlock Text="Персональные данные" FontSize="20" FontWeight="Bold" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                    <TextBlock Text="Бизнес поездки" FontSize="20" FontWeight="Bold" HorizontalAlignment="Right" VerticalAlignment="Top"/>
                </Grid>
            </StackPanel>
        </Border>

    </Grid>
</Window>
